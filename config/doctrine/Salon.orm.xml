<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Salon" table="salon">
    <indexes>
      <index name="fk_salon_decoracion1_idx" columns="decoracion_idDecoracion"/>
    </indexes>
    <id name="idsalon" type="integer" column="idSalon">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombresalon" type="string" column="nombreSalon" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="capacidad" type="string" column="capacidad" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="foto" type="string" column="foto" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="precio" type="float" column="precio" precision="10" scale="0" nullable="false"/>
    <many-to-one field="decoracionIddecoracion" target-entity="Decoracion" fetch="LAZY">
      <join-columns>
        <join-column name="decoracion_idDecoracion" referenced-column-name="idDecoracion"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
